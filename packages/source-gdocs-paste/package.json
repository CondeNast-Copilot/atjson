  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@atjson/document": "^0.7.11",
    "@atjson/schema": "^0.7.9"
  }
}
