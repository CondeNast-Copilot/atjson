{
  "lerna": "2.2.0",
  "version": "0.8.7",
  "packages": [
    "packages/@atjson/*"
  ],
  "command": {
    "publish": {
      "conventionalCommits": true,
      "changelogPreset": "@atjson/conventional-commits"
    },
    "bootstrap": {
      "hoist": true,
      "npmClientArgs": [
        "--no-package-lock"
      ]
    },
    "publish": {
      "exact": true
    }
  }
}
